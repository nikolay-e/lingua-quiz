{
  "name": "@lingua-quiz/frontend",
  "version": "1.0.0",
  "description": "Frontend application for Lingua Quiz",
  "type": "module",
  "scripts": {
    "predev": "npm run build --workspace=@lingua-quiz/core",
    "dev": "vite",
    "build": "npm run build --workspace=@lingua-quiz/core && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "dependencies": {
    "@lingua-quiz/core": "file:../core"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tsconfig/svelte": "^5.0.5",
    "svelte": "^5.39.4",
    "svelte-check": "^4.3.2",
    "svelte-preprocess": "^6.0.3",
    "vite": "^7.1.7"
  },
  "keywords": [
    "svelte",
    "frontend",
    "language-learning",
    "quiz"
  ]
}
