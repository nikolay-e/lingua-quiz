{
  "threshold": 3,
  "reporters": ["console"],
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.venv/**",
    "**/venv/**",
    "**/env/**",
    "**/__pycache__/**",
    "**/generated/**",
    "**/.git/**"
  ],
  "format": ["javascript", "typescript", "python"],
  "minLines": 6,
  "minTokens": 50
}
